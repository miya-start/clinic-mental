---
import appleTouchIcon from '~/assets/favicons/apple-touch-icon.png';
import fav16 from '~/assets/favicons/favicon-16x16.png';
import fav32 from '~/assets/favicons/favicon-32x32.png';
import favIcon from '~/assets/favicons/favicon.ico';
import maskIcon from '~/assets/favicons/safari-pinned-tab.svg';
---

<link rel="shortcut icon" href={favIcon} />
<link rel="icon" type="image/png" sizes="16x16" href={fav16.src} />
<link rel="icon" type="image/png" sizes="32x32" href={fav32.src} />
<link rel="mask-icon" href={maskIcon.src} color="#5bbad5" />
<link rel="apple-touch-icon" sizes="180x180" href={appleTouchIcon.src} />
<meta name="msapplication-TileColor" content="#da532c" />
<meta name="theme-color" content="#ffffff" />
